{
    "name": "intraworlds/phpunit-db-fixtures",
    "description": "Loading DB fixtures with ease",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ondřej Ešler",
            "email": "ondrej.esler@intraworlds.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "phpunit/phpunit": "^11.0"
    },
    "suggest": {
        "opensearch-project/opensearch-php" : "Needed for OpenSearch support",
        "elasticsearch/elasticsearch" : "Needed to support Elasticsearch",
        "mongodb/mongodb" : "Needed to support MongoDB",
        "symfony/yaml": "For YAML fixtures"
    },
    "autoload": {
        "psr-4": {
            "IW\\PHPUnit\\DbFixtures\\": "src"
        }
    },
    "require-dev": {
        "robmorgan/phinx": "^0.12",
        "mongodb/mongodb": "^1.5",
        "symfony/yaml": "^7",
        "rector/rector": "^1.2"
    }
}
